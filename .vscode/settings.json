    "cmake.configureArgs": [
        "-DENABLE_ASAN=ON",
        //"-DUSE_LOCAL_OBJECTS=OFF",
        "-DDISABLE_HA=ON",
        "-DDEV_MODE=ON",
        "-DENABLE_SIP=ON",
        "-DTEST_ENABLED=OFF"
    ],
    "cmake.generator": "Unix Makefiles",
    "cmake.parallelJobs": 8,
    "clangd.arguments": [
        // "--resource-dir=/usr/bin/",
        "-log=verbose",
        "-pretty",
        "--background-index",
        "--compile-commands-dir=${workspaceFolder}/build",
	    "-header-insertion=never",
